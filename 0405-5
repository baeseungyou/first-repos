package abcde;

public class EX5 {
	public static void main (String args[]) {
		int i = 1;
		int cnt=0, sum=0, mul=0;
		
		i=1;
		while (i<=100) 
		{
			if(i%mul==0)
			{
				System.out.printf ("%4d", i);
				cnt++;
				sum +=i;
				if(cnt%7 == 0)
					System.out.println();
			}
			i++;
			}
			System.out.println("\n "+ mul + "의 배수의 개수는 " + cnt + "입니다");
			System.out.println("합은 " + sum + "입니다");
		}
	}



		
