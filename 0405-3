package abcde;

public class Ex3 {
	public static void main(String[] args) {
		int i = 1;
		int cnt=0, sum=0;
		
		for (i = 1; i<=100; i++) 
		{
			if(i%5==0)
			{
				System.out.printf ("%4d", i);
				cnt++;
				sum +=i;
				if(cnt%7 == 0)
					System.out.println();
			}
		}
			System.out.println("\n5의 배수의 개수는 " + cnt + "입니다");
			System.out.println("합은 " + sum + "입니다");
		}
	}


